{
  "platibus.rabbitmq": {
    "baseUri": "amqp://guest:guest@localhost:5672/",
    "replyTimeout": "00:00:30",
    "endpoints": {
      "loopback": { "address": "amqp://guest:guest@localhost:5672/" }
    },
    "sendRules": [
      {
        "namePattern": ".*",
        "endpoint": "loopback"
      }
    ]
  }
}